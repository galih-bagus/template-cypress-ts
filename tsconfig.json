{
   "compilerOptions": {
      "target": "ES2020",
      "lib": ["ES2020", "DOM"],
      "module": "Node16",
      "moduleResolution": "Node16",
      "types": ["cypress"],
      "baseUrl": ".",
      "paths": {
         "@helper/*": ["test/helper/*"],
         "@test/*": ["test/*"]
      },
      "strict": true,
      "skipLibCheck": true
   },
   "include": ["cypress/**/*.ts", "test/**/*.ts", "cypress.config.ts"]
}
